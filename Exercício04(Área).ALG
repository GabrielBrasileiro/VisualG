Algoritmo "Área"

Var

   // Seção de Declarações das variáveis
   raio: real
   area: real

Inicio

   // Seção de Comandos, procedimento, funções, operadores, etc...

   escreval("Digite o valor do raio em metros: ")
   leia(raio)

   area <- (raio^2 * PI)

   escreva("O valor da área é: ", area," metros quadrados")


Fimalgoritmo